{
  "functions": {
    "app/api/inngest/route.js": {
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/inngest",
      "destination": "/api/inngest/route"
    }
  ],
  "headers": [
    {
      "source": "/api/inngest",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, X-Inngest-Signature, Content-Type" }
      ]
    }
  ]
}